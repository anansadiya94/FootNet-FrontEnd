# Uncomment the next line to define a global platform for your project
platform :ios, '12.2'
use_frameworks!

target 'FootNet' do
  
  # Pods for FootNet
  pod 'CocoaLumberjack/Swift' #Logger
  pod 'Material'
  pod 'lottie-ios'
  pod 'Swinject'
  
  target 'FootNetTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'CocoaLumberjack/Swift' #Logger
    pod 'Material'
    pod 'lottie-ios'
    pod 'Swinject'
  end

  target 'FootNetUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'CocoaLumberjack/Swift' #Logger
    pod 'Material'
    pod 'lottie-ios'
    pod 'Swinject'
  end

end
